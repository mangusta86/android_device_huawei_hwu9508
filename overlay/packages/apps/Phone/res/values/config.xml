<!-- Phone app resources that may need to be customized
     for different hardware or product builds. -->
<resources>

    <!-- Flag indicating if the phone is a world phone -->
    <bool name="world_phone">true</bool>
    
 
    <!-- Flag indicating if hac is enabled-->
    <bool name="hac_enabled">false</bool>
	 <!--Determine whether calls to mute the microphone in PhoneUtils
         are routed through the android.media.AudioManager class (true) or through
         the com.android.internal.telephony.Phone interface (false).-->
    <bool name="send_mic_mute_to_AudioManager">false</bool>
    <!-- Determines if device implements a noise suppression device for in call audio-->
    <bool name="has_in_call_noise_suppression">false</bool>
    <!-- Audio parameter for setting noise suppression-->
    <string name="in_call_noise_suppression_audioparameter">noise_suppression=on=off</string>
    <!-- Voice quality filter parameter
         Set this if your device's libaudio has a parameter to adjust voice quality
         (don't forget to also overlay to value array in that case).
         Leave empty to disable the feature. -->
    <string name="voice_quality_param"></string>


</resources>
