				          <mnclst>2, 5,7</mnclst>
                        </plmnid>
                        <cellid>148</cellid> <!-- Valid values  0 to 2 pow 28-->
                    </cellglobalid>
                    <trackareacode>54</trackareacode>  <!-- Valid values  0 to 2 pow 16 -->
                </cgiinfo>
                <meas-res-valid>1</meas-res-valid>
                <meas-res>
                    <!-- Indicates if RSRP measurement is valid -->
                    <rsrp-valid>1</rsrp-valid>
